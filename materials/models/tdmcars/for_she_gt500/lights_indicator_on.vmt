"VertexlitGeneric"
{
	"$basetexture" "models/tdmcars/for_she_gt500/lights_indicator_mask"
	"$bumpmap" "models/tdmcars/shared/skin_nm"
	"$selfillum" "1"
	"$selfillumtint" "[10 3 1]"
	"$selfillumfresnel" "1"
	"$selfillumfresnelminmaxexp" "[.2 2.0 10]"
	$envmap "env_cubemap"
	$envmaptint "[.1 .1 .1]"
	"$colorfix" "{255 255 255}"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedTextureVar" "$basetexture"
			"animatedTextureFrameNumVar" "$frame"
			"animatedTextureFrameRate" "6"
		}
		"Equals"
		{
			srcVar1 $colorfix
			resultVar	$color
		}		
	}
}

